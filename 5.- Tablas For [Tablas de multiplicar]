#include <stdio.h>
#include <stdlib.h>

int main() {
    int a;
    int b;
    for(a=1; a<=10; a++){  /*Aqui estamos indicando que nuestra tabla de multiplicación está dentro del 
    intervalo del 1 al 10, el cual podemos modificar para que llegue al intervalo deseado, si asi se quiere*/
        printf("\nTabla de multiplicación del %d\n",a);
        for(b=1; b<=10; b++){
            printf("%d x %d=%d\n", a, b, a*b);
        }
    }
}
